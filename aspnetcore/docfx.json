{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/sample/**",
          "**/samples/**",
          "aspnet/**",
          "**/includes/**",
          "**/aspnet-core-conceptual/**",
          "_site/**"
        ],
        "group": "group1",
        "src": "."
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "aspnet/core/**",
          "**/includes/**",
          "_site/**"
        ],
        "group": "group1",
        "src": "."
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "bilingual_type": "hover over",
      "breadcrumb_path": "/aspnet/breadcrumb/toc.json",
      "_navPath": "/foo",
      "_navRel": "/foo",
      "searchScope": ["ASP.NET Core"]
    },
    "fileMetadata": {},
    "template": [],
    "dest": "_site",
    "groups": {
      "group1": {
        "dest": "group1-dest",
        "moniker_range": ">= aspnetcore-1.0"
      }
    }  
  }
}
